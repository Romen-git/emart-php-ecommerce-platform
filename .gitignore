# Ignore environment and configuration files
.env

# Ignore log files and temporary files
*.log
*.tmp
*.cache

# Ignore IDE specific files
.vscode/
.idea/

# Ignore OS specific files
.DS_Store
Thumbs.db

# Ignore compiled CSS if using pre-processors like SASS/SCSS
*.sass-cache/
*.scss-cache/

# Ignore files generated by PHPUnit tests
phpunit.xml
phpunit.xml.dist
phpunit-result.cache

# Ignore PHPStorm project specific metadata
.phpstorm.meta.php

# Ignore compiled files
/public/build/

# Ignore cache, session, and logs (Laravel/other PHP frameworks)
storage/logs/*
storage/framework/sessions/*
storage/framework/cache/*
storage/framework/views/*

# Ignore compiled blade views (Laravel/other PHP frameworks)
*.blade.php

# Ignore coverage reports
coverage/
